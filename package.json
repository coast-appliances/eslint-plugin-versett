{
  "name": "@versett/eslint-plugin-versett",
  "version": "0.0.0-semantically-released",
  "description": "A tool for linting projects with different techs",
  "main": "index.js",
  "scripts": {
    "precommit": "versett-devtools precommit",
    "preparecommitmsg": "versett-devtools preparecommitmsg ${GIT_PARAMS}",
    "release": "yarn && versett-devtools release --npm-publish",
    "test": "jest",
    "test:ci": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versett/eslint-plugin-versett.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/versett/eslint-plugin-versett/issues"
  },
  "homepage": "https://github.com/versett/eslint-plugin-versett#readme",
  "devDependencies": {
    "@versett/versett-devtools": "^1.9.0",
    "jest": "^23.6.0"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "getLastRelease": "last-release-git"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": ">= 5.6.0 < 6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3"
  }
}
